<div class="container">
    <div class="row">
        <div class="row">
            <div class="col-md-9">
                <h1>
                    Conozca Nuestros Productos</h1>
            </div>
            
        </div>
        <div id="carousel-example" class="carousel slide hidden-xs" data-ride="carousel">
            <!-- Wrapper for slides -->
            <div class="carousel-inner">
                <div class="item active">
                    <div class="row">
                        <%  @products = Product.limit(20) %>
                            <% @products.each do |product| %>

                        <div class="col-sm-3">
                            <div class="col-item">
                                <div class="photo">
                                <center><%= link_to image_tag(product.avatar.url(:thumb)), @product, :remote => true,  :rel => "tooltip", :title => "Mostrar", :class => 'btn btn-link img-responsive' %></center>
                                </div>
                                <div class="info">
                                    <div class="row">
                                        <div class="price col-md-6">
                                            <h5 class="label label-info">
                                                <%= product.name %></h5>
                                            <h5 class="price-text-color" style="margin-top:10px; !important">
                                                $199.99</h5>
                                        </div>
                                        <div class="rating hidden-sm col-md-6" style="top:20px;">
                                            <i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star">
                                            </i><i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star">
                                            </i><i class="fa fa-star"></i>
                                        </div>
                                    </div>
                                    <div class="separator clear-left">
                                        <p class="btn-add">
                                            <%= link_to 'Ver Datos', product , :class => 'btn btn-primary'%><i class="fa fa-shopping-cart"></i></p>
                                        <p class="btn-details">
                                            <a href="http://www.jquery2dotnet.com" class="btn btn-success">Add to cart<i class="fa fa-shopping-cart"></i></a></p>
                                    </div>
                                    <div class="clearfix">
                                    </div>
                                </div>
                            </div>
                        <br/>
                        </div>

                        <% end %>
                    </div>
                </div>

            </div>
        </div>
    </div>

</div>
















