<tr id=<%= "manage_user_#{manage_user.id}" %>>
	<td><%= link_to manage_user.id, edit_manage_user_path(manage_user), :remote => true %></td>
	<td><%= manage_user.user.email %></td>
	<td><%= manage_user.rol.name %></td>
	
	<td>
		<%= link_to  image_tag('Options/show.png'), manage_user, :remote => true,  :rel => "tooltip", :title => "Mostrar", :class => 'btn btn-link'%>
		<%= link_to image_tag('Options/edit.png'), edit_manage_user_path(manage_user),  :remote => true,  :rel => "tooltip", :title => "Editar", :class => 'btn btn-link'%>
	</td>
</tr>