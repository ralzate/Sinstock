<%- model_class = ManageUser -%>
<button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick="$.lazybox.close()">&times;</button>
<div class="page-header">
  <h3 align="center">Registro numero: <%= @manage_user.id %></h3>
  <table class="table table-bordered table-hover" >
    <thead>
      <tr class="active">
        <th>Usuario</th>
        <th>Rol</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><p><%= @manage_user.user.first_name %></p></td>
        <td><p><%= @manage_user.rol.name %></p></td>
      </tr>
    </tbody>
  </table>
  <div class="modal-footer">
    <a href="#" class="btn btn-primary" onclick="$.lazybox.close()">Cerrar</a>
  </div>
</div>