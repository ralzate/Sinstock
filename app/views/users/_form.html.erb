<div id="div1" class="oculto">
    <div class="editar" id="persona">
<div class="oculto-en-moviles">

<%= form_for(@user, html: { multipart: true }) do |f| %>

    <div class="user">
      <div class="modal-header">
        <h4 class="modal-title"><h3>Datos para personas</h3></h4>
      </div><br>
        <div class="row">
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Primer_Nombre %>
                    <%= f.text_field :first_name, class: "form-control" %>
                </div>
            </div>
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Segundo_Nombre %>
                    <%= f.text_field :second_name, class: "form-control" %>
                </div>
            </div>
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Identificacion %>
                    <%= f.text_field :identification, class: "form-control" %>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Primer_Apellido %>
                    <%= f.text_field :first_surname, class: "form-control" %>
                </div>
            </div>
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Segundo_Apellido %>
                    <%= f.text_field :second_surname, class: "form-control"  %>
                </div>
            </div>
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Telefono %>
                    <%= f.text_field :phone, class: "form-control" %>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Direccion %>
                    <%= f.text_field :address, class: "form-control" %>
                </div>
            </div>
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Celular %>
                    <%= f.text_field :cell_phone, class: "form-control" %>
                </div>
            </div>
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Rut %>
                    <%= f.text_field :rut, class: "form-control" %>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Pagina_Web %>
                    <%= f.text_field :web_page, class: "form-control", :label => "Pagina Web:" %>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <div class="col-sm-10">
                    <%= f.label :Primer_Nombre %>
                    <%= f.text_field :first_name_of_contact_person, class: "form-control", autofocus: true, :label => "Primer nombre:" %>
                </div>
            </div>
            <div class="col-md-3">
                <div class="col-sm-10">
                    <%= f.label :Primer_Nombre %>
                    <%= f.text_field :second_name_of_contact_person, class: "form-control", :label => "Segundo nombre:"  %>
                </div>
            </div>
            <div class="col-md-3">
                <div class="col-sm-10">
                    <%= f.label :Primer_Apellido %>
                    <%= f.text_field :first_surname_of_contact_person, class: "form-control", :label => "Primer Apellido:" %>
                </div>
            </div>
            <div class="col-md-3">
                <div class="col-sm-10">
                    <%= f.label :Segundo_Apellido %>
                    <%= f.text_field :second_surname_of_contact_person, class: "form-control", :label => "Segundo Apellido:" %>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Email %>
                    <%= f.email_field :email, class: "form-control"  %>
                </div>
            </div>
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Clave %>
                    <%= f.password_field :password, class: "form-control" %>
                </div>
            </div>
            <div class="col-md-4">
                <div class="col-sm-10">
                    <%= f.label :Confirmar_clave %>
                    <%= f.password_field :password_confirmation, class: "form-control" %>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="col-sm-10">
                    <%= f.label :Primer_Telefono %>
                    <%= f.text_field :first_phone_of_contact_person, class: "form-control" %>
                </div>
            </div>
            <div class="col-md-6">
                <div class="col-sm-10">
                    <%= f.label :Segundo_Telefono %>
                    <%= f.text_field :second_phone_of_contact_person, class: "form-control"  %>
                </div>
            </div>
            <%= f.hidden_field :type_user, value: "Persona" %>

        </div>
        <br/><br/>
        <%= f.submit 'Enviar!', class: 'btn btn-primary btn-lg' %>
    </div>
<% end %>
</div>
</div>

</div>
