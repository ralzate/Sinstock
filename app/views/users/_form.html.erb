




<script type="text/javascript">
    

    jQuery(document).ready(function(){ 
    $("#hide1").click(function(){
        $("#hide2").hide();
    });
    $("#hide2").click(function(){
        $("#hide1").hide();
    });
    $(".oculto").hide();              
    $(".MO").click(function(){
          var nodo = $(this).attr("href");  
 
          if ($(nodo).is(":visible")){
               $(nodo).hide();
               return false;
          }else{
        $(".oculto").hide();                             
        $(nodo).fadeToggle( "slow" );
        return false;
          }
    });
}); 
</script>


